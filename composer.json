{
  "name": "laurisaarni/we_author",
  "description": "Gives you the possibility to add author informations into every page or tx_news entry",
  "version": "1.0",
  "autoload": {
    "psr-4": {
      "WE\\Weauthor\\": "Classes/"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": ">7.6.0,<8.99.99||dev-master",
    "lochmueller/focuspoint": "~2.1.0"
  },
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3 CMS",
    "We Author"
  ],
  "homepage": "https://github.com/laurisaarni/we_author",
  "license": "GPL-2.0+",
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "Package": {
        "partOfMinimalUsableSystem": true
      }
    }
  }
}
